{
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "declaration": true,
      "module": "commonjs",
      "newLine": "lf",
      "noEmitOnError": true,
      "noImplicitAny": false,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "sourceMap": true,
      "strict": true,
      "strictNullChecks": true,
      "target": "es2015",
      "lib": ["es2015"]
    }
  }