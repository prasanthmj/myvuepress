{
  "name": "myvuepress",
  "version": "1.0.1",
  "license": "UNLICENSED",
  "workspaces": [
    "plugins/*",
    "themes/*"
  ],
  "private": true,
  "devDependencies": {
    "@vuepress/plugin-google-analytics": "^1.2.0",
    "lerna": "^3.19.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "typescript": "^3.6.3"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap --use-workspaces",
    "version-upd":"lerna version patch",
    "show-website-1": "vuepress dev ./websites/website1",
    "form-guide-dev": "vuepress dev ./websites/form-guide/docs",
    "form-guide-build": "vuepress build ./websites/form-guide/docs",
    "form-guide-deploy": "cd ./websites/form-guide && netlify deploy --prod --dir=./docs/.vuepress/dist",
    "jsc-dev": "vuepress dev ./websites/jsc/docs",
    "jsc-build": "vuepress build ./websites/jsc/docs",
    "jsc-deploy": "cd ./websites/jsc && netlify deploy --dir=./docs/.vuepress/dist",
    "build": "lerna run build --stream",
    "release": "lerna  publish  --registry http://localhost:4873 --no-git-tag-version"
  },
  "publishConfig": {
    "registry": "http://localhost:4873"
  },
  "dependencies": {
    "slugify": "^1.3.6"
  }
}
